{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "pivot-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pivotWebPart/PivotWebPartWebPart.js",
          "manifest": "./src/webparts/pivotWebPart/PivotWebPartWebPart.manifest.json"
        }
      ]
    },
    "panel-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/panelWebPart/PanelWebPartWebPart.js",
          "manifest": "./src/webparts/panelWebPart/PanelWebPartWebPart.manifest.json"
        }
      ]
    },
    "details-list-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/detailsListWebPart/DetailsListWebPartWebPart.js",
          "manifest": "./src/webparts/detailsListWebPart/DetailsListWebPartWebPart.manifest.json"
        }
      ]
    },
    "command-bar-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/commandBarWebPart/CommandBarWebPartWebPart.js",
          "manifest": "./src/webparts/commandBarWebPart/CommandBarWebPartWebPart.manifest.json"
        }
      ]
    },
    "one-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/onePart/OnePartWebPart.js",
          "manifest": "./src/webparts/onePart/OnePartWebPart.manifest.json"
        }
      ]
    },
    "editable-grid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/editableGrid/EditableGridWebPart.js",
          "manifest": "./src/webparts/editableGrid/EditableGridWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HomeWebPartStrings": "lib/webparts/home/loc/{locale}.js",
    "PivotWebPartStrings": "lib/webparts/pivot/loc/{locale}.js",
    "PivotWebPartWebPartStrings": "lib/webparts/pivotWebPart/loc/{locale}.js",
    "PanelWebPartWebPartStrings": "lib/webparts/panelWebPart/loc/{locale}.js",
    "DetailsListWebPartWebPartStrings": "lib/webparts/detailsListWebPart/loc/{locale}.js",
    "CommandBarWebPartWebPartStrings": "lib/webparts/commandBarWebPart/loc/{locale}.js",
    "OnePartWebPartStrings": "lib/webparts/onePart/loc/{locale}.js",
    "EditableGridWebPartStrings": "lib/webparts/editableGrid/loc/{locale}.js"
  }
}
